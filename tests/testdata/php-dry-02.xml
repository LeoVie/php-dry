<?xml version="1.0" encoding="UTF-8"?>
<php-dry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../xsd/php-dry.xsd"
         silent="true"
         minTokenLength="60"
         minSimilarTokensPercentage="75"
         enableLcsAlgorithm="true"
         countOfParamSets="15"
         enableConstructNormalization="true"
         enableCleanCodeScoring="false"
         phpDocumentorReportPath="report_of_phpdoc"
         phpDocumentorExecutablePath="bla_foo"
         cachePath="/tmp/">

    <report>
        <cli/>
        <json filepath="foo/bar/report.json"/>
    </report>
</php-dry>
